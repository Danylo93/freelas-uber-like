{
  "name": "@freelas/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:wait": "node dist/server.js",
    "db:push": "prisma db push --schema=src/shared/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/shared/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/shared/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema=src/shared/database/prisma/schema.prisma",
    "kafka:topics": "bash scripts/create-kafka-topics.sh",
    "kafka:check": "bash scripts/check-kafka.sh"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "kafkajs": "^2.2.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tsyringe": "^4.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
